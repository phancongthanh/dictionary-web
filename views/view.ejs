<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- font google -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@300;500&display=swap" rel="stylesheet">
    <!-- search.css  -->
    <link rel="stylesheet" href="/css/search.css">
    <title>Search</title>
</head>

<body>
    <header>
        <div class="container">
            <nav class="navbar">
                <!-- logo  -->
                <div class="navbar-logo">
                    <a href="/"><img src="/image/logodesign.png" alt="logo web"></a>
                </div>

                <h1 class="header-title">English Dictionary</h1>
                <!-- navbar-linls  -->
                <ul class="navbar-links">
                    <li class="navbar-link"><a href="/add">Add</a></li>
                    <li class="navbar-link"><a href="/edit">Edit</a></li>

                </ul>
                <!-- menu buttons  -->
                <div class="navbar-icons">
                    <div class="navbar-icon"></div>
                </div>
            </nav>
            <!-- <h1 class="header-title">English Dictionary</h1> -->
        </div>
    </header>
    <section id="body">
        <div class="container">
            <!-- input text  -->
            <div class="body-inputs">
                <input type="text" class="body-input" placeholder="Searching Your Word" id="searchword">
                <div class="dropbox">
                    <li>abcxyz</li>
                    <li>abcxyz</li>
                    <li>abcxyz</li>
                    <li>abcxyz</li>
                    <li>abcxyz</li>
                    <li>abcxyz</li>
                </div><br>
                <!-- button search  -->
                <button class="search-button" onclick="getClick()">Search</button>
                <!-- onclick="getClick()" -->
            </div>
        </div>
    </section>
    <section id="out-put">
        <div class="container-output">
            <!-- output text  -->
            <ul class="output">
                <!---- <output type="text" class="output-text"></output> -->
                <li class="word">
                    <div class="detail">
                        <p id="word-out">word</p>

                    </div>
                </li>
                <li class="element">
                    <div class="detail">
                        <p id="type-out">Noun </p>
                       <p id="pronunciation-out">/wɜːd/</p>

                    </div>
                </li>
                <li class="meaning" id="VN-meaning">
                    <div class="detail">
                        <p>Vietnamese meaning:</p>
                        <span id="vn-out">từ , cuộc nói chuyện ,tin tức </span>
                    </div>
                </li>
                <li class="meaning" id="EN-meaning">
                    <div class="detail">
                        <p>English-meaning:</p>
                        <span id="en-out"> a single unit of language that means something and can be spoken or
                            written</span>
                    </div>
                </li>
                <li class="meaning" id="example">
                    <div class="detail">
                        <p>Example:</p>
                        <span id="example-out"> Do you know the words to this song?</span>
                    </div>
                </li>
                <li class="synonym">
                    <div class="detail">
                        <p>Synonym</p>
                        <div class="synonym-list">
                            <p id="synonym-out">syn1, syn2 ,syn3</p>

                        </div>

                    </div>
                </li>
                <div class="delete-btn-container">
                    <button class="del-btn">delete</button>
                </div>
            </ul>

        </div>

    </section>
    <!-- icon  -->
    <script src="https://kit.fontawesome.com/650d0c074b.js" crossorigin="anonymous"></script>
    <!-- main.js  -->
    <script src="/js/search.js"></script>
    <script src="/js/suggestions.js"></script>
</body>

</html>